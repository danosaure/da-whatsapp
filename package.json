    "@babel/core": "~7.7.7",
    "@babel/preset-env": "~7.7.7",
    "@babel/preset-react": "~7.7.4",
    "@quoin/react-utils": "~0.0.4",
    "@quoin/react-utils-bs3": "~1.0.0",
    "babel-loader": "~8.0.6",
